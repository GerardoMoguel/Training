<Window x:Class="InventarioRestaurante.AltaIngrediente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventarioRestaurante"
        mc:Ignorable="d"
        Title="AltaIngrediente" Height="398.2" Width="337.05" Background="#FF4E4E4E">
    <Grid>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="29" Margin="177,103,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120" Background="#FF383838" Foreground="#FFBCBCBC" SelectionBrush="#FF00D9EE" FontSize="14" TextAlignment="Center"/>
        <TextBox x:Name="txtCantidad" HorizontalAlignment="Left" Height="30" Margin="177,190,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120" Background="#FF383838" Foreground="#FFBCBCBC" SelectionBrush="#FF00D9EE" FontSize="14" TextAlignment="Center"/>
        <Button x:Name="btDarDeAlta" Content="Alta" HorizontalAlignment="Left" Margin="196,287,0,0" VerticalAlignment="Top" Width="85" Click="btDarDeAlta_Click" Background="#FFB3F5FF" Height="34" FontSize="14" FontFamily="Segoe UI Semibold"/>
        <TextBox x:Name="txtDescripcion" HorizontalAlignment="Left" Height="29" Margin="177,146,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120" Background="#FF383838" Foreground="#FFBCBCBC" SelectionBrush="#FF00D9EE" FontSize="14" TextAlignment="Center"/>
        <TextBox x:Name="txtId_Restaurante" HorizontalAlignment="Left" Height="29" Margin="177,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FF383838" Foreground="#FFBCBCBC" SelectionBrush="#FF00D9EE" FontSize="14" TextAlignment="Center"/>
        <TextBox x:Name="txtPrecio" HorizontalAlignment="Left" Height="29" Margin="177,238,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="120" Background="#FF383838" Foreground="#FFBCBCBC" SelectionBrush="#FF00D9EE" FontSize="14" TextAlignment="Center"/>
        <Button x:Name="btn_atras" Content="Atras" HorizontalAlignment="Left" Margin="10,334,0,0" VerticalAlignment="Top" Width="66" Click="btn_atras_Click" Background="#FFD1B3FF" FontFamily="Segoe UI Light" Height="25"/>
        <StackPanel HorizontalAlignment="Left" Height="248" Margin="33,60,0,0" VerticalAlignment="Top" Width="139"
                    
                    
                    >
            <Label x:Name="lbId_Restaurante" Content="Id_Restaurante" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Height="43" Foreground="#FFDADADA"/>
            <Label x:Name="lbNombreIngre" Content="Nombre" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Height="43" Foreground="#FFDADADA"/>
            <Label x:Name="lbOrigen" Content="Descripcion" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Height="44" Foreground="#FFDADADA"/>
            <Label x:Name="lbCantIngre" Content="Cantidad" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Height="43" Foreground="#FFDADADA"/>
            <Label x:Name="lbPrecio" Content="Precio" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Height="48" Foreground="#FFDADADA"/>
        </StackPanel>
        <Label x:Name="lbId_Restaurante_Copy" Content="Alta de ingrediente." HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="16" Height="43" Foreground="#FFDADADA" Margin="84,12,0,0" FontFamily="Segoe UI Semibold"/>

    </Grid>
</Window>
