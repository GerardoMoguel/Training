        <TextBox x:Name="txtId_Ingrediete" HorizontalAlignment="Left" Height="29" Margin="127,66,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="126" RenderTransformOrigin="-1.443,-1.675" Foreground="#FFBCBCBC" Background="#FF383838" SelectionBrush="#FFB3C4FF"/>
        <Label Content="ID Ingrediente" HorizontalAlignment="Left" Margin="24,66,0,0" VerticalAlignment="Top" Foreground="#FFDADADA" FontSize="14"/>
